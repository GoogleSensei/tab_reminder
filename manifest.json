{
"manifest_version": 2,
"name": "tab reminder",
"description": "for your productivity",
"version": "1.0",
"icons": {
"man": "19man.png"
},
"background": {
    "scripts": ["src/js/background.js"]
},
"browser_action": {
    "default_icon": {
        "19": "19man.png"
    },
    "default_title": "Tab Reminder",
    "default_popup": "popup.html"
},
"permissions": [
"Tabs",
"unlimitedStorage",
"notifications"
]
}
