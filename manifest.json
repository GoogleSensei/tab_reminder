{
"manifest_version": 2,
"name": "tab reminder",
"description": "for your productivity",
"version": "1.0",
"icons": {
"19": "/icons/19man.png"
},
"background": {
    "scripts": ["src/js/background.js"],
    "persistent": true
},
"browser_action": {
    "default_icon": {
        "19": "/icons/19man.png"
    },
    "default_title": "Tab Reminder",
    "default_popup": "/src/html/popup.html"
},
"permissions": [
"tabs",
"unlimitedStorage",
"notifications", 
"http://*/*", 
"https://*/*"
]
}
